citHeader("To cite wiml in publications use:")

citEntry(entry = "Article",
  title        = "Transforming Feature Space to Interpret Machine Learning Models",
  author       = personList(as.person("Alexander Brenning")),
  year         = "2021",
  journal      = "arXiv preprint",
  pages        = "arXiv:2104.04295",

  textVersion  =
  paste("Brenning, A. (2021).",
        "Transforming Feature Space to Interpret Machine Learning Models.",
        "arXiv preprint, arXiv:2104.04295")
)
